<Window x:Class="PL.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:PL"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <!--Button to go to TaskListWindow-->
        <Button Content="Handle Tasks" HorizontalAlignment="Left" Margin="200,192,0,0" Height="40" Width="100" Background="Orange" VerticalAlignment="Top" Click="showTaskList_Click" />
        <Button Content="Initialize Database" Height="40" Width="100" HorizontalAlignment="Center" Margin="0,291,0,0" VerticalAlignment="Top" Background="LightBlue" RenderTransformOrigin="0.96,0.713" Click="initializeData_Click"/>
        <Button Content="Handle Engineers" HorizontalAlignment="Left" Height="40" Width="100"  VerticalAlignment="Top" Margin="500,192,0,0" Background="Orange" Click="showEngineers_Click" RenderTransformOrigin="-0.514,0.668"/>

    </Grid>
</Window>
